#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision - 12;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_005BE130 .scope module, "testbench_add2" "testbench_add2" 2 2;
 .timescale -9 -12;
v00B2D898_0 .var "a", 3 0;
v00B2D8F0_0 .var "b", 3 0;
v00B23CF0_0 .var "clk", 0 0;
v00B23D48_0 .var "correct_ov", 0 0;
v00B23DA0_0 .var "correct_sum", 3 0;
v00B231F8_0 .net "ov", 0 0, C4<0>; 1 drivers
v00B23250_0 .var "rst", 0 0;
v00B232A8_0 .net "sum", 3 0, L_005C0800; 1 drivers
v00B23300_0 .var "test_num", 6 0;
E_00B2A510/0 .event negedge, v00B23250_0;
E_00B2A510/1 .event posedge, v00B23CF0_0;
E_00B2A510 .event/or E_00B2A510/0, E_00B2A510/1;
S_005BE1B8 .scope module, "DUT" "add2" 2 16, 3 1, S_005BE130;
 .timescale 0 0;
v00B2AEC0_0 .net "a", 3 0, v00B2D898_0; 1 drivers
v00B2ACC0_0 .net "b", 3 0, v00B2D8F0_0; 1 drivers
v00B2DB10_0 .alias "ov", 0 0, v00B231F8_0;
v00B2D840_0 .alias "sum", 3 0, v00B232A8_0;
L_005C0800 .arith/sum 4, v00B2D898_0, v00B2D8F0_0;
    .scope S_005BE130;
T_0 ;
    %delay 5000, 0;
    %load/v 8, v00B23CF0_0, 1;
    %inv 8, 1;
    %set/v v00B23CF0_0, 8, 1;
    %jmp T_0;
    .thread T_0;
    .scope S_005BE130;
T_1 ;
    %delay 5000, 0;
    %load/v 8, v00B23250_0, 1;
    %inv 8, 1;
    %set/v v00B23250_0, 8, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_005BE130;
T_2 ;
    %ix/load 0, 1, 0;
    %assign/v0 v00B23CF0_0, 0, 0;
    %ix/load 0, 1, 0;
    %assign/v0 v00B23250_0, 0, 0;
    %ix/load 0, 4, 0;
    %assign/v0 v00B2D898_0, 0, 0;
    %ix/load 0, 4, 0;
    %assign/v0 v00B2D8F0_0, 0, 0;
    %ix/load 0, 7, 0;
    %assign/v0 v00B23300_0, 0, 0;
    %vpi_call 2 25 "$dumpfile", "add2.vcd";
    %vpi_call 2 26 "$dumpvars";
    %end;
    .thread T_2;
    .scope S_005BE130;
T_3 ;
    %wait E_00B2A510;
    %load/v 8, v00B23250_0, 1;
    %jmp/0xz  T_3.0, 8;
    %load/v 8, v00B2D898_0, 4;
    %mov 12, 0, 2;
   %cmpi/u 8, 15, 6;
    %or 5, 4, 1;
    %jmp/0xz  T_3.2, 5;
    %load/v 8, v00B2D898_0, 4;
    %mov 12, 0, 28;
    %addi 8, 1, 32;
    %ix/load 0, 4, 0;
    %assign/v0 v00B2D898_0, 0, 8;
    %load/v 8, v00B23300_0, 7;
    %mov 15, 0, 25;
    %addi 8, 1, 32;
    %ix/load 0, 7, 0;
    %assign/v0 v00B23300_0, 0, 8;
T_3.2 ;
    %load/v 8, v00B2D898_0, 4;
    %mov 12, 0, 2;
    %cmpi/u 8, 15, 6;
    %jmp/0xz  T_3.4, 4;
    %ix/load 0, 4, 0;
    %assign/v0 v00B2D898_0, 0, 0;
    %load/v 8, v00B2D8F0_0, 4;
    %mov 12, 0, 28;
    %addi 8, 1, 32;
    %ix/load 0, 4, 0;
    %assign/v0 v00B2D8F0_0, 0, 8;
T_3.4 ;
    %jmp T_3.1;
T_3.0 ;
    %load/v 8, v00B2D898_0, 4;
    %mov 12, 0, 1;
    %load/v 13, v00B2D8F0_0, 4;
    %mov 17, 0, 1;
    %add 8, 13, 5;
    %set/v v00B23DA0_0, 8, 4;
    %set/v v00B23D48_0, 12, 1;
    %load/v 8, v00B232A8_0, 4;
    %load/v 12, v00B231F8_0, 1;
    %load/v 13, v00B23DA0_0, 4;
    %load/v 17, v00B23D48_0, 1;
    %cmp/u 8, 13, 5;
    %jmp/0xz  T_3.6, 4;
    %vpi_call 2 44 "$display", "Test %d ", v00B23300_0;
    %vpi_call 2 45 "$display", "OK!";
    %vpi_call 2 46 "$display", "%d + %d = ?", v00B2D898_0, v00B2D8F0_0;
    %vpi_call 2 47 "$display", "your answer: ov = %d, sum = %d", v00B231F8_0, v00B232A8_0;
    %vpi_call 2 48 "$display", "correct answer: ov = %d, sum = %d", v00B231F8_0, v00B232A8_0;
    %vpi_call 2 49 "$display", "\012";
    %jmp T_3.7;
T_3.6 ;
    %vpi_call 2 52 "$display", "Test %d ", v00B23300_0;
    %vpi_call 2 53 "$display", "/////////////////////";
    %vpi_call 2 54 "$display", "////////Fail!////////";
    %vpi_call 2 55 "$display", "/////////////////////";
    %vpi_call 2 56 "$display", "%d + %d = ?", v00B2D898_0, v00B2D8F0_0;
    %vpi_call 2 57 "$display", "your answer: ov = %d, sum = %d", v00B231F8_0, v00B232A8_0;
    %vpi_call 2 58 "$display", "correct answer: ov = %d, sum = %d", v00B23D48_0, v00B23DA0_0;
    %vpi_call 2 59 "$display", "\012";
T_3.7 ;
T_3.1 ;
    %jmp T_3;
    .thread T_3;
    .scope S_005BE130;
T_4 ;
    %delay 1005000, 0;
    %vpi_call 2 63 "$finish";
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "testbench_add2.v";
    "add2.v";
