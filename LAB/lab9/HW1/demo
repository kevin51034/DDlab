    %load/v 8, v003ACC10_0, 16;
    %ix/load 0, 16, 0;
    %assign/v0 v003ACBB8_0, 0, 8;
T_1.5 ;
T_1.3 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_003AB010;
T_2 ;
