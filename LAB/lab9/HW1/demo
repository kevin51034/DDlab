    .scope S_003AAF00;
T_3 ;
    %vpi_call 2 27 "$dumpfile", "lab9_hw_unsigned.vcd";
    %vpi_call 2 28 "$dumpvars";
    %set/v v005B30C0_0, 0, 1;
    %delay 20000, 0;
    %set/v v005E9D18_0, 1, 1;
    %movi 8, 3, 8;
    %set/v v005E9D70_0, 8, 8;
    %movi 8, 9, 8;
    %set/v v005E9DC8_0, 8, 8;
    %delay 20000, 0;
    %set/v v005E9D18_0, 0, 1;
    %delay 700000, 0;
    %set/v v005E9D18_0, 1, 1;
    %movi 8, 25, 8;
    %set/v v005E9D70_0, 8, 8;
    %set/v v005E9DC8_0, 0, 8;
    %delay 20000, 0;
    %set/v v005E9D18_0, 0, 1;
    %delay 700000, 0;
    %set/v v005E9D18_0, 1, 1;
    %movi 8, 100, 8;
    %set/v v005E9D70_0, 8, 8;
    %movi 8, 14, 8;
    %set/v v005E9DC8_0, 8, 8;
    %delay 20000, 0;
    %set/v v005E9D18_0, 0, 1;
    %delay 700000, 0;
    %set/v v005E9D18_0, 1, 1;
    %movi 8, 123, 8;
    %set/v v005E9D70_0, 8, 8;
    %movi 8, 12, 8;
    %set/v v005E9DC8_0, 8, 8;
    %delay 20000, 0;
    %set/v v005E9D18_0, 0, 1;
    %delay 700000, 0;
    %set/v v005E9D18_0, 1, 1;
    %movi 8, 45, 8;
    %set/v v005E9D70_0, 8, 8;
    %movi 8, 69, 8;
    %set/v v005E9DC8_0, 8, 8;
    %delay 20000, 0;
    %set/v v005E9D18_0, 0, 1;
    %delay 20000, 0;
    %set/v v005E9D18_0, 0, 1;
    %delay 700000, 0;
    %vpi_call 2 59 "$finish";
    %end;
    .thread T_3;
    .scope S_003AAF00;
T_4 ;
    %delay 10000, 0;
    %load/v 8, v005B30C0_0, 1;
    %inv 8, 1;
    %set/v v005B30C0_0, 8, 1;
    %jmp T_4;
