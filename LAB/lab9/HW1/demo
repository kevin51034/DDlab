    %movi 32, 0, 8;
    %mul 8, 24, 16;
    %ix/load 0, 16, 0;
    %assign/v0 v003AF228_0, 0, 8;
    %load/v 8, v003AF280_0, 6;
    %mov 14, 0, 26;
    %addi 8, 1, 32;
    %ix/load 0, 6, 0;
    %assign/v0 v003AF280_0, 0, 8;
    %load/v 8, v005E9CC0_0, 1;
    %mov 9, 0, 2;
    %cmpi/u 8, 1, 3;
    %jmp/0xz  T_5.2, 4;
    %load/v 8, v005E9C68_0, 16;
    %load/v 24, v003AF228_0, 16;
    %cmp/u 8, 24, 16;
    %inv 4, 1;
    %jmp/0xz  T_5.4, 4;
    %ix/load 0, 1, 0;
    %assign/v0 v003AF2D8_0, 0, 1;
    %vpi_call 2 86 "$display";
