E_005B0108 .event posedge, v003AD5C0_0, v003AD6E8_0;
    .scope S_003AB010;
T_0 ;
    %wait E_005B0108;
    %load/v 8, v003AD5C0_0, 1;
    %jmp/0xz  T_0.0, 8;
    %ix/load 0, 6, 0;
    %assign/v0 v003AAAA8_0, 0, 0;
    %jmp T_0.1;
T_0.0 ;
    %ix/load 0, 1, 0;
    %load/vp0 8, v003AAAA8_0, 6;
    %ix/load 0, 6, 0;
    %assign/v0 v003AAAA8_0, 0, 8;
T_0.1 ;
    %jmp T_0;
