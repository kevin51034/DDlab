    .scope S_003AB010;
T_1 ;
    %wait E_005B0108;
    %load/v 8, v003AD5C0_0, 1;
    %jmp/0xz  T_1.0, 8;
    %ix/load 0, 16, 0;
    %assign/v0 v003ACC10_0, 0, 0;
    %ix/load 0, 16, 0;
    %assign/v0 v003ACBB8_0, 0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/v 8, v003AAAA8_0, 6;
    %cmpi/u 8, 0, 6;
    %jmp/0xz  T_1.2, 4;
    %load/v 8, v005B3068_0, 8;
    %mov 16, 0, 8;
    %ix/load 0, 16, 0;
    %assign/v0 v003ACC10_0, 0, 8;
    %load/v 8, v005B3068_0, 8;
    %mov 16, 0, 8;
    %ix/load 0, 16, 0;
    %assign/v0 v003ACBB8_0, 0, 8;
    %jmp T_1.3;
T_1.2 ;
    %load/v 8, v003AAAA8_0, 6;
   %cmpi/u 8, 8, 6;
    %or 5, 4, 1;
    %jmp/0xz  T_1.4, 5;
    %load/v 8, v003ACBB8_0, 1; Only need 1 of 16 bits
; Save base=8 wid=1 in lookaside.
    %jmp/0xz  T_1.6, 8;
    %mov 8, 0, 8;
    %load/v 16, v005B3010_0, 8;
    %load/v 24, v003ACC10_0, 16;
    %add 8, 24, 16;
    %set/v v003ACC10_0, 8, 16;
T_1.6 ;
    %load/v 8, v003ACC10_0, 16;
    %ix/load 0, 1, 0;
    %mov 4, 0, 1;
    %shiftr/i0  8, 16;
    %set/v v003ACBB8_0, 8, 16;
    %load/v 8, v003ACBB8_0, 16;
    %set/v v003ACC10_0, 8, 16;
    %jmp T_1.5;
T_1.4 ;
    %load/v 8, v003ACC10_0, 16;
    %ix/load 0, 16, 0;
